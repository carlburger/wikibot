{
    "$schema": "https://statics.teams.microsoft.com/sdk/v1.0/manifest/MicrosoftTeams.schema.json",
    "manifestVersion": "1.0",
    "version": "1.0.0",
    "id": "%APP_GUID%",
    "packageName": "com.carlburger.wikibot",
    "developer": {
        "name": "Carl Burger",
        "websiteUrl": "https://wikibot.glitch.me/",
        "privacyUrl": "https://wikibot.glitch.me/",
        "termsOfUseUrl": "https://wikibot.glitch.me/"
    },
    "name": {
        "short": "Wikibot"
    },
    "description": {
        "short": "Searches wikipedia for keywords and posts abstracts",
        "full": "This bot searches Wikipedia for given keywords and posts back the abstract paragraph if a page is found. If not it displays possible alternatives as found by the Wikipoedia search."
    },
    "icons": {
        "outline": "wikibot20x20.png",
        "color": "wikibot96x96.png"
    },
    "accentColor": "#60A18E",
    "bots": [
        {
            "botId": "%BOT_FRAMEWORK_ID%",
            "scopes": [
                "team",
                "personal"
            ]
        }
    ],
    "permissions": [
        "identity",
        "messageTeamMembers"
    ]
}
